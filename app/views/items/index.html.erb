<h1>Index results</h1>
<p>This is where the cards will be</p>

<%= render 'items/search_form' %>

<ul>
<% @items.each do |item| %>
  <li>
    <h3><%= item.name %></h3>
    <p><%= item.address %></p>
    <p><%= item.credits %> credits</p>
    <p><%= link_to "show", item_path(item) %></p>
  </li>
<% end %>
</ul>
